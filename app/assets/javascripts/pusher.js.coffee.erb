
$ ->
  pusher = new Pusher("<%= Pusher.key %>") # uses your APP KEY
  channel = pusher.subscribe("frc862")
  channel.bind "info", (data) ->
    $(".live-info > div:hidden").remove()
    killme = $(".live-info > div").slice(7)
    $("<div>#{data.info}</div>").clone().hide().prependTo('.live-info').slideDown()
    killme.fadeOut()
