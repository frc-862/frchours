- @title = "Member List"

= link_to "xml", reports_hours_path(format: :xml)
= link_to "json", reports_hours_path(format: :json)
.sheet.sheet-condensed
  .sheet-inner
    %table.table
      %thead
        %tr
          %th Last name
          %th First name
          %th Student ID
          %th Grade
          %th Email
      %tbody
      - @members.each do |member|
        %tr
          %td= member.last_name
          %td= member.first_name
          %td= member.student_id
          %td= member.grade
          %td= member.email

