- @title = "Member List"

.sheet.sheet-condensed
  .sheet-inner
    %table.table
      %thead
        %tr
          %th Last name
          %th First name
          %th Grade
          %th Email
      %tbody
      - @members.each do |member|
        %tr
          %td= member.last_name
          %td= member.first_name
          %td= member.grade
          %td= member.email

